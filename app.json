{
  "expo": {
    "name": "Ujenzi Bridge",
    "slug": "ujenzi-bridge",
    "version": "1.0.5",

    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ],
      [
        "expo-facebook",
        {
          "userTrackingPermission": false
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#FE9A01",
      "resizeMode": "contain"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true,
      "androidMode": "default"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "ujenzibridge.co.ke",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to capture and upload user profile image and Listing Image to the server.",
        "NSPhotoLibraryUsageDescription": "This app needs access to the photo library to select and upload user profile image and Listing Image to the server.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to the photo library to select and upload user profile image and Listing Image to the server.",
        "NSLocationWhenInUseUsageDescription": "This app usages device location on listing information, Listing search Data and User information.",
        "CFBundleAllowMixedLocalizations": true,
        "UIBackgroundModes": ["remote-notification"]
      },
      "config": {
        "googleMapsApiKey": "AIzaSyCs0TJfggT2wARP0noZInG_2bLSVyGX5GE"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.ujenzi.bridge",
      "versionCode": 5,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCs0TJfggT2wARP0noZInG_2bLSVyGX5GE"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fff"
      },
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "MEDIA_LIBRARY",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "READ_PHONE_STATE",
        "READ_CONTACTS",
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "facebookScheme": "fb248887017031234",
    "facebookAppId": "248887017031234",
    "facebookDisplayName": "Your App",
    "scheme": "yourapp"
  }
}
